function [ predictions, SSE ] = arppredictions( a, p, winsize )
%UNTITLED Summary of this function goes here
%   Detailed explanation goes here
    amean=mean(a);
    a=a-amean;
    %alen=len(a);
    x=zeros(winsize-p:p);
    disp(x);
    y=zeros(winsize-p,1);
    disp(size(y));
    for i=p:winsize-1
        x(i-p+1,:)=a(i-p+1:i);
        y(i-p+1)=a(i+1);
    end
    disp(y);
    disp(x);

   coeffic=regress(y,x);
   
end

