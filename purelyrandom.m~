function [ predictions ] = purelyrandom( a, winsize )
%UNTITLED2 Summary of this function goes here
%   Detailed explanation goes here
    N=length(a);
    predsize=N-winsize;
    amean=mean(a);
    %a=a-amean;
    predictions=zeros(predsize, 1);
    for k=0:predsize-1
       
        predictions(k+1)=a(winsize+k);
    end

end

